<SuccessModel name="Chat Service Success Model" service="i5.las2peer.services.chatService.ChatService">
	
	<dimension name="System Quality">
		<factor name="Performance">
			<measure name="Average Time to send Message in Milliseconds"/>
			<measure name="Average Time to fetch Chatroom Information in Milliseconds"/>
		</factor>
	</dimension>
	
	<dimension name="Information Quality">
		<factor name="Quantity">
			<measure name="Number of public Chatrooms over Time"/>
			<measure name="Number of private Chatrooms over Time"/>
		</factor>
		<factor name="Quality">
			<measure name="Average Users per public Chatroom"/>
			<measure name="Average Users per private Chatroom"/>
		</factor>
	</dimension>
	
	<dimension name="Use">
		<factor name="Active Use">
			<measure name="Send Messages over Time"/>
			<measure name="Average Messages send per User"/>
		</factor>
		<factor name="Passive Use">
			<measure name="Received Messages over Time"/>
			<measure name="Average Messages received per User"/>
		</factor>
	</dimension>
	
	<dimension name="User Satisfaction">
		<factor name="Time">
			<measure name="Average Time spend in Chatroom in Seconds"/>
		</factor>
	</dimension>
	
	<dimension name="Individual Impact">
		
	</dimension>
	
	<dimension name="Community Impact">
		
	</dimension>
	
</SuccessModel>