<NodeSuccessModel name="Node Success Model">
	<dimension name="System Quality">
		<factor name="Stability">
			<measure name="Successful Agent Uploads Ratio"/>
		</factor>
	</dimension>
	<dimension name="Information Quality">
		<factor name="Activity">
			<measure name="Service Registrations"/>
			<measure name="User Registrations"/>
			<measure name="User Registrations over Time"/>
		</factor>
	</dimension>
	<dimension name="Use">
		<factor name="Uploaded Entities">
			<measure name="Agent Uploads"/>
			<measure name="Added Artifacts"/>
		</factor>
		<factor name="Messages">
			<measure name="Send LAS2peer Messages over Time"/>
		</factor>
	</dimension>
	<dimension name="User Satisfaction">
		<factor name="Time spend Online">
			<measure name="Average Time Users Logged-In in Seconds"/>
		</factor>
	</dimension>
	<dimension name="Individual Impact">
		<factor name="No Factors or Measures yet">
		</factor>
	</dimension>
	<dimension name="Community Impact">
		<factor name="No Factors or Measures yet">
		</factor>
	</dimension>
</NodeSuccessModel>